{
    "name": "seb/authenticator-bundle",
    "description": "Symfony SebAuthenticatorBundle",
    "keywords": [
        "User authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiano Degan",
            "email": "sebdeg87@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-imap": "*",
        "symfony/security-bundle": "^3.0 || ^4.0 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seb\\AuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seb\\AuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
